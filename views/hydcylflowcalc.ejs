<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Design Calculations</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
  <body class='bg-dark fs-5'>
	<%- include('navbar') %>
	<h2 class='text-center text-light'>Hydraulic Cylnder Flow Calculations<h2>
	<div class="container text-center">
		<table class="table table-dark teble-striped text-light fs-6">
			<thead>
				<tr>
					<th class="text-light">Project</th>
					<th>Power Pack</th>
					<th>Valve Stand</th>
					<th>Equipment</th>
					<th>Function</th>
					<th>Action</th>
				</tr>
			</thead>
			<tbody>
				<% if (rows.length < 1) { %>
					<tr>
						<td colspan=6>No Calaculations available</td>
					</tr>
				<% } else { %>
					<% rows.map((row)=>{ %>
						<tr>
							<td><%= row.project %></td>
							<td><%= row.powerpack %></td>
							<td><%= row.valvestand %></td>
							<td><%= row.equipment %></td>
							<td><%= row.fun %></td>
							<td>
								<a href="/hydcylflowcalcshow/<%= row.id %>"><button class="btn btn-secondary btn-sm">Show</button></a>
							</td>
						</tr>
					<% }) %>;
				<% } %>
			</tbody>
		</table>
		<div class="text-end">
			<a href='/hydcylflowcalcnew'><button class="btn btn-secondary">Add Calculation</button></a>
		</div>
	</div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>